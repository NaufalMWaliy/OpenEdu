<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record model="ir.ui.view" id="organization_history_form_view">
		<field name="name">organization.history.form</field>
		<field name="model">oe.organization.history</field>
		<field name="arch" type="xml">
			<form string="Educational History Form">
				<sheet>
					<group>
						<field name="name" placeholder="Example.. Himpunan Mahasiswa Komputer, Jurusan Teknik Komputer"/>
						<field name="year" placeholder="Example.. '2016 to 2016'"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record model="ir.ui.view" id="educational_history_tree_view">
		<field name="name">organization.history.form</field>
		<field name="model">oe.organization.history</field>
		<field name="arch" type="xml">
			<tree string="Educational History Tree">
				<field name="year"/>
				<field name="name"/>
			</tree>
		</field>
	</record>

	<record model="ir.actions.act_window" id="organization_history_form_action">
		<field name="name">organization.history.form</field>
		<field name="res_model">oe.organization.history</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>
</odoo>