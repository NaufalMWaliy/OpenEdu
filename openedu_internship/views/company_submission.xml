<?xml version="1.0" encoding="UTF-8"?>
<odoo>

<!-- Form View -->
  <record id="view_company_submission_form" model="ir.ui.view">
            <field name="name">view.form.company.submission</field>
            <field name="model">oe.company</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="form Company Submission">
                    <sheet>
						<group col="4">
						<field name="nama" colspan="4" nolabel="1"/>
						</group>
						<group col="4" colspan="4">
                            <field name="date" />
							<field name="date" />
                        </group>

						<notebook colspan="4">
                        <page string="Company Identity">
						<group col="4" colspan="4">
							<field name="nama_perusahaan" />
							<field name="email" />
							<field name="official_website" />
							<field name="nama_contact_person" />
						</group>
						</page>
						</notebook>
                    </sheet>
                </form>
            </field>
        </record>
  <!-- Tree View -->
  <record id="pengajuan_perusahaan_tree" model="ir.ui.view" >
    <field name="name">CompanySubmission</field>
    <field name="model">oe.company</field>
    <field name="arch" type="xml">
      <tree>
		<field name="nama" />
      </tree>
    </field>
  </record>

</odoo>